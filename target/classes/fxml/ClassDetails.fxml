          <TableColumn fx:id="functionName" prefWidth="142.0" text="Function Name" />
          <TableColumn fx:id="visibility" prefWidth="128.0" text="access modifier" />
            <TableColumn fx:id="returnType" prefWidth="123.0" text="Return Type" />
            <TableColumn fx:id="linesOfCode" prefWidth="96.0" text="Lines Of Code" />
        </columns>
      </TableView>
      <Label layoutX="54.0" layoutY="102.0" text="List Of Functions">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Button layoutX="54.0" layoutY="20.0" mnemonicParsing="false" onAction="#navigateBack" text="Back" />
