<AnchorPane id="AnchorPane" prefHeight="561.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.asu.ser.render.ClassDetailsController">
    <stylesheets>
        <URL value="@/styles/classdetails.css" />
    </stylesheets>
   <children>
      <Label layoutX="139.0" layoutY="14.0" text="Details of Class :">
         <font>
            <Font size="25.0" />
